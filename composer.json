{
  "name": "zoglo/contao-legacy-templates",
  "type": "contao-bundle",
  "description": "Restores the contao legacy content elements in Contao 5",
  "keywords": [
    "contao 5",
    "html",
    "legacy",
    "templates"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Zoglowek",
      "homepage": "https://github.com/zoglo",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.2"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/contao-demo": "*"
  },
  "autoload": {
    "psr-4": {
      "Zoglo\\ContaoLegacyTemplates\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "contao-manager-plugin": "Zoglo\\ContaoLegacyTemplates\\ContaoManager\\Plugin"
  }
}
